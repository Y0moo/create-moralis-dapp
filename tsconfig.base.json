{
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "compilerOptions": {
    "target": "es2015",
    "sourceMap": true,
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "build",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "jest"
    ],
    "lib": [
      "es2019"
    ],
    "declaration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noPropertyAccessFromIndexSignature": false,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@create-moralis-dapp/next": [
        "packages/next/src/index.ts"
      ],
      "@create-moralis-dapp/react": [
        "packages/react/src/index.ts"
      ],
      "@create-moralis-dapp/toolkit": [
        "packages/toolkit/src/index.ts"
      ],
      "create-eth-dapp": [
        "packages/create-eth-dapp/src/index.ts"
      ]
    }
  }
}